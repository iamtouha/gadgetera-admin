{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "name": "address"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "receiver": {
      "type": "string"
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "district": {
      "type": "string"
    },
    "sub_district": {
      "type": "string"
    },
    "street_address": {
      "type": "text"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "address",
      "private": true
    }
  }
}
