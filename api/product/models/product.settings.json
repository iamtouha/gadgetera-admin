{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 10
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "sku": {
      "type": "string",
      "private": false,
      "required": true,
      "unique": true,
      "minLength": 4
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "brand": {
      "model": "brand"
    },
    "price": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "stock": {
      "type": "boolean",
      "default": true
    },
    "discount": {
      "type": "decimal",
      "default": 0,
      "max": 1,
      "min": 0
    },
    "subcategory": {
      "model": "subcategory",
      "via": "products"
    },
    "overview": {
      "type": "text"
    },
    "model": {
      "type": "string"
    }
  }
}
